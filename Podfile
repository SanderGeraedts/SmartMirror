# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'SmartMirror' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SmartMirror
  pod 'SwiftyJSON'
  pod 'Alamofire'
  pod 'SVProgressHUD'

  target 'SmartMirrorTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SmartMirrorUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
